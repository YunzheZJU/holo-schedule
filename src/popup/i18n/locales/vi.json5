    reminder: {
      // Prompt users to set the reminder for a scheduled live
      set: "Nhắc tôi",
      // Prompt users to cancel the reminder
      cancel: "Hủy nhắc"
    },
    // Used to customize dayjs#calendar output
    calendar: {
