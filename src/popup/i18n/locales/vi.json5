          ko: "한국어",
          pl: "Polski",
          'pt-PT': "Português (Portugal)",
          'pt-BR': "Português (Brasil)",
          ru: "Русский",
          sv: "Svenska",
          th: "ไทย",
          vi: "Tiếng Việt",
          'zh-CN': "简体中文",
          'zh-TW': "繁體中文"
        }
