      newDebuts: "Nya debuter!",
      hololive: "Hololive",
      hololiveDEVIS: "Hololive DEV_IS",
      holostars: "HOLOSTARS",
      hololiveChina: "Hololive Kina",
      hololiveIndonesia: "Hololive Indonesien",
      hololiveEnglish: "Hololive English",
      holostarsEnglish: "HOLOSTARS English",
      holoAN: "holoAN"
    }
  },
  // Component MemberGroup
  memberGroup: {
    // Help users to perform group operations when detected
    selectHelper: {
      // Prompt users to unsubscribe to all members in a group or not, with the question mark
      mute: "Stäng av ljudet för denna grupp?",
      // Prompt users to subscribe to all members in a group or not, with the question mark
      subscribe: "Prenumerera på denna grupp?",
